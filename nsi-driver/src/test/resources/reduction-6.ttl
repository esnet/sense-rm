@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .

owl:FunctionalProperty
  rdfs:subClassOf  rdfs:Resource .

owl:InverseFunctionalProperty
  rdfs:subClassOf  rdfs:Resource .

owl:OntologyProperty  rdfs:subClassOf  rdfs:Resource .

rdfs:ContainerMembershipProperty
  rdfs:subClassOf  rdfs:Resource .

rdf:List  a                  owl:Class ;
          rdfs:subClassOf      rdf:List ;
          owl:equivalentClass  rdf:List .

owl:TransitiveProperty
  rdfs:subClassOf  rdfs:Resource .

owl:ObjectProperty  rdfs:subClassOf  rdfs:Resource .

owl:DatatypeProperty  rdfs:subClassOf  rdfs:Resource .

owl:Ontology  a              owl:Class ;
              rdfs:subClassOf      owl:Ontology ;
              owl:equivalentClass  owl:Ontology .

owl:SymmetricProperty
  rdfs:subClassOf  rdfs:Resource .

rdf:Statement  a             owl:Class ;
               rdfs:subClassOf      rdf:Statement ;
               owl:equivalentClass  rdf:Statement .

owl:Property  a              owl:Class ;
              rdfs:subClassOf      owl:Property ;
              owl:equivalentClass  owl:Property .

rdfs:Literal  a              owl:Class ;
              rdfs:subClassOf      rdfs:Literal ;
              owl:equivalentClass  rdfs:Literal .