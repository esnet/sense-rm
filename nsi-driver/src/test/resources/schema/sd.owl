<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns="http://schemas.ogf.org/nsi/2013/12/services/definition#"
        xml:base="http://schemas.ogf.org/nsi/2013/12/services/definition"
    
        xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
        xmlns:owl="http://www.w3.org/2002/07/owl#"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
        xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
        xmlns:nml="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
        xmlns:sd="http://schemas.ogf.org/nsi/2013/12/services/definition#">

    <owl:Ontology rdf:about="http://schemas.ogf.org/nsi/2013/12/services/definition#">
        <rdfs:label>Open Grid Forum NSI Connection Services Protocol v2.0 - Service definition template definition.</rdfs:label>
    </owl:Ontology>

    <owl:Class rdf:about="http://schemas.ogf.org/nsi/2013/12/services/definition#ServiceDefinition">
        <rdfs:subClassOf rdf:resource="http://schemas.ogf.org/nml/2013/03/base#NetworkObject"/>
    </owl:Class>
    
    <owl:ObjectProperty rdf:about="http://schemas.ogf.org/nsi/2013/12/services/definition#hasServiceDefinition">
        <rdfs:domain rdf:resource="http://schemas.ogf.org/nml/2013/03/base#NetworkObject"/>
        <rdfs:range rdf:resource="http://schemas.ogf.org/nsi/2013/12/services/definition#ServiceDefinition"/>
    </owl:ObjectProperty>

    <owl:DatatypeProperty rdf:about="http://schemas.ogf.org/nsi/2013/12/services/definition#comment">
        <rdfs:domain rdf:resource="http://schemas.ogf.org/nsi/2013/12/services/definition#ServiceDefinition"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://schemas.ogf.org/nsi/2013/12/services/definition#serviceType">
        <rdfs:domain rdf:resource="http://schemas.ogf.org/nsi/2013/12/services/definition#ServiceDefinition"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    
</rdf:RDF>

